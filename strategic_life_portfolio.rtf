\cb3         \}\cb1 \
\
\cb3         // --- MATRIX CHART COMPONENT (Enhanced) ---\cb1 \
\cb3         function MatrixChart(\{ data, structure, isPdf = false \}) \{\cb1 \
\cb3             const width = 800;\cb1 \
\cb3             const height = 800;\cb1 \
\cb3             const padding = 60;\cb1 \
\cb3             const chartSize = width - padding * 2;\cb1 \
\cb3             \cb1 \
\cb3             const [hoveredUnit, setHoveredUnit] = useState(null);\cb1 \
\
\cb3             const xScale = (val) => padding + (val / 10) * chartSize;\cb1 \
\cb3             const yScale = (val) => height - (padding + (val / 10) * chartSize); \cb1 \
\cb3             const rScale = (val) => 10 + (val * 4); \cb1 \
\
\cb3             // Theme Colors for PDF vs Screen\cb1 \
\cb3             const axisColor = isPdf ? '#4b5563' : 'currentColor'; // gray-600 vs current\cb1 \
\cb3             const gridColor = isPdf ? '#e5e7eb' : 'currentColor'; // gray-200 vs current\cb1 \
\cb3             const textColor = isPdf ? '#111827' : 'currentColor'; // gray-900 vs current\cb1 \
\cb3             const bubbleStroke = isPdf ? 'white' : (document.documentElement.classList.contains('dark') ? '#000' : '#fff');\cb1 \
\
\cb3             const bubbles = useMemo(() => \{\cb1 \
\cb3                 const b = [];\cb1 \
\cb3                 structure.forEach(group => \{\cb1 \
\cb3                     group.units.forEach(unit => \{\cb1 \
\cb3                         const unitData = data[unit.id] || \{ importance: 5, satisfaction: 5, timeInvested: 5 \};\cb1 \
\cb3                         b.push(\{\cb1 \
\cb3                             ...unitData,\cb1 \
\cb3                             ...unit,\cb1 \
\cb3                             color: group.color,\cb1 \
\cb3                             cx: xScale(unitData.satisfaction),\cb1 \
\cb3                             cy: yScale(unitData.importance),\cb1 \
\cb3                             r: rScale(unitData.timeInvested)\cb1 \
\cb3                         \});\cb1 \
\cb3                     \});\cb1 \
\cb3                 \});\cb1 \
\cb3                 return b;\cb1 \
\cb3             \}, [data, structure]);\cb1 \
\
\cb3             // Calculate collisions and layout\cb1 \
\cb3             const labelData = useMemo(() => \{\cb1 \
\cb3                 return bubbles.map((current, i) => \{\cb1 \
\cb3                     const isOverlapping = bubbles.some((other, j) => \{\cb1 \
\cb3                         if (i === j) return false;\cb1 \
\cb3                         const dist = Math.hypot(current.cx - other.cx, current.cy - other.cy);\cb1 \
\cb3                         return dist < (current.r + other.r + 5); \cb1 \
\cb3                     \});\cb1 \
\
\cb3                     const isSmall = current.r < 28; \cb1 \
\cb3                     const showOutside = isOverlapping || isSmall;\cb1 \
\
\cb3                     if (showOutside) \{\cb1 \
\cb3                         const centerX = width / 2;\cb1 \
\cb3                         const centerY = height / 2;\cb1 \
\cb3                         const angle = Math.atan2(current.cy - centerY, current.cx - centerX);\cb1 \
\cb3                         \cb1 \
\cb3                         const lineLen = 40; \cb1 \
\cb3                         const labelX = current.cx + Math.cos(angle) * (current.r + lineLen);\cb1 \
\cb3                         const labelY = current.cy + Math.sin(angle) * (current.r + lineLen);\cb1 \
\cb3                         \cb1 \
\cb3                         const startX = current.cx + Math.cos(angle) * current.r;\cb1 \
\cb3                         const startY = current.cy + Math.sin(angle) * current.r;\cb1 \
\
\cb3                         const anchor = labelX > current.cx ? "start" : "end";\cb1 \
\cb3                         const xOffset = labelX > current.cx ? 5 : -5;\cb1 \
\
\cb3                         return \{ ...current, type: 'outside', lx: labelX, ly: labelY, sx: startX, sy: startY, anchor: anchor, xOffset: xOffset \};\cb1 \
\cb3                     \} else \{\cb1 \
\cb3                         return \{ ...current, type: 'inside' \};\cb1 \
\cb3                     \}\cb1 \
\cb3                 \});\cb1 \
\cb3             \}, [bubbles]);\cb1 \
\
\cb3             return (\cb1 \
\cb3                 <div className=\{`w-full aspect-square max-w-4xl mx-auto relative font-sans $\{isPdf ? '' : ''\}`\}>\cb1 \
\cb3                     \{/* Hover Card (Not shown in PDF) */\}\cb1 \
\cb3                     \{!isPdf && (\cb1 \
\cb3                         <AnimatePresence>\cb1 \
\cb3                             \{hoveredUnit && (() => \{\cb1 \
\cb3                                  const unit = labelData.find(u => u.id === hoveredUnit);\cb1 \
\cb3                                  if(!unit) return null;\cb1 \
\cb3                                  const energyPct = (unit.timeInvested / 10) * 100;\cb1 \
\cb3                                  const isRightSide = unit.cx > width / 2;\cb1 \
\cb3                                  const isBottomSide = unit.cy > height / 2;\cb1 \
\cb3                                  const cardStyle = \{\cb1 \
\cb3                                      left: isRightSide ? 'auto' : `$\{(unit.cx / width) * 100\}%`,\cb1 \
\cb3                                      right: isRightSide ? `$\{100 - ((unit.cx / width) * 100)\}%` : 'auto',\cb1 \
\cb3                                      top: isBottomSide ? 'auto' : `$\{(unit.cy / height) * 100\}%`,\cb1 \
\cb3                                      bottom: isBottomSide ? `$\{100 - ((unit.cy / height) * 100)\}%` : 'auto',\cb1 \
\cb3                                      marginLeft: isRightSide ? 0 : '20px',\cb1 \
\cb3                                      marginRight: isRightSide ? '20px' : 0,\cb1 \
\cb3                                      marginTop: isBottomSide ? 0 : '20px',\cb1 \
\cb3                                      marginBottom: isBottomSide ? '20px' : 0\cb1 \
\cb3                                  \};\cb1 \
\
\cb3                                  return (\cb1 \
\cb3                                     <motion.div \cb1 \
\cb3                                         initial=\{\{ opacity: 0, scale: 0.8, y: 10 \}\}\cb1 \
\cb3                                         animate=\{\{ opacity: 1, scale: 1, y: 0 \}\}\cb1 \
\cb3                                         exit=\{\{ opacity: 0, scale: 0.8, y: 10 \}\}\cb1 \
\cb3                                         transition=\{\{ duration: 0.2 \}\}\cb1 \
\cb3                                         className="absolute z-50 w-64 bg-black/90 dark:bg-black/90 bg-neutral-900/95 backdrop-blur-lg border border-white/20 shadow-2xl rounded-xl overflow-hidden pointer-events-none font-sans"\cb1 \
\cb3                                         style=\{cardStyle\}\cb1 \
\cb3                                     >\cb1 \
\cb3                                         <div className="absolute top-0 left-0 w-full h-1 bg-gradient-to-r from-indigo-500 via-purple-500 to-pink-500"></div>\cb1 \
\cb3                                         <div className="p-4">\cb1 \
\cb3                                             <div className="flex justify-between items-start mb-2">\cb1 \
\cb3                                                 <h3 className="text-sm font-bold text-white">\{unit.name\}</h3>\cb1 \
\cb3                                                 <span className="text-[10px] font-mono text-white/50 bg-white/10 px-1.5 py-0.5 rounded">\{unit.area\}</span>\cb1 \
\cb3                                             </div>\cb1 \
\cb3                                             <p className="text-xs text-white/70 mb-4 leading-snug">\{unit.desc\}</p>\cb1 \
\cb3                                             <div className="space-y-3">\cb1 \
\cb3                                                 <div className="grid grid-cols-2 gap-2">\cb1 \
\cb3                                                     <div className="bg-white/5 p-2 rounded border border-white/5">\cb1 \
\cb3                                                         <div className="flex items-center gap-1 text-white/50 text-[10px] uppercase tracking-wider mb-1"><Star size=\{10\} /> Importance</div>\cb1 \
\cb3                                                         <div className="text-lg font-bold text-indigo-400">\{unit.importance\}<span className="text-xs text-white/30 font-normal">/10</span></div>\cb1 \
\cb3                                                     </div>\cb1 \
\cb3                                                     <div className="bg-white/5 p-2 rounded border border-white/5">\cb1 \
\cb3                                                         <div className="flex items-center gap-1 text-white/50 text-[10px] uppercase tracking-wider mb-1"><Heart size=\{10\} /> Satisfaction</div>\cb1 \
\cb3                                                         <div className="text-lg font-bold text-emerald-400">\{unit.satisfaction\}<span className="text-xs text-white/30 font-normal">/10</span></div>\cb1 \
\cb3                                                     </div>\cb1 \
\cb3                                                 </div>\cb1 \
\cb3                                                 <div className="pt-2 border-t border-white/10">\cb1 \
\cb3                                                     <div className="flex justify-between items-center text-xs mb-1">\cb1 \
\cb3                                                         <span className="flex items-center text-white/80"><Zap size=\{12\} className="mr-1 text-amber-400" /> Time Invested</span>\cb1 \
\cb3                                                         <span className="font-mono text-amber-400">\{unit.timeInvested\}h+</span>\cb1 \
\cb3                                                     </div>\cb1 \
\cb3                                                     <div className="w-full h-1.5 bg-white/10 rounded-full overflow-hidden">\cb1 \
\cb3                                                         <motion.div initial=\{\{ width: 0 \}\} animate=\{\{ width: `$\{energyPct\}%` \}\} className="h-full bg-gradient-to-r from-amber-500 to-orange-500" />\cb1 \
\cb3                                                     </div>\cb1 \
\cb3                                                 </div>\cb1 \
\cb3                                             </div>\cb1 \
\cb3                                         </div>\cb1 \
\cb3                                     </motion.div>\cb1 \
\cb3                                  );\cb1 \
\cb3                             \})()\}\cb1 \
\cb3                         </AnimatePresence>\cb1 \
\cb3                     )\}\cb1 \
\
\cb3                     <svg viewBox=\{`0 0 $\{width\} $\{height\}`\} className=\{`w-full h-full rounded-lg border shadow-inner $\{isPdf ? 'bg-transparent border-none' : 'bg-neutral-50 dark:bg-black/40 border-neutral-200 dark:border-white/10'\}`\}>\cb1 \
\cb3                         <defs>\cb1 \
\cb3                             <pattern id=\{isPdf ? "grid-pdf" : "grid"\} width=\{chartSize/10\} height=\{chartSize/10\} patternUnits="userSpaceOnUse">\cb1 \
\cb3                                 <path d=\{`M $\{chartSize/10\} 0 L 0 0 0 $\{chartSize/10\}`\} fill="none" stroke=\{gridColor\} className=\{isPdf ? '' : 'text-neutral-200 dark:text-white/5'\} strokeWidth="1"/>\cb1 \
\cb3                             </pattern>\cb1 \
\cb3                         </defs>\cb1 \
\cb3                         <rect x=\{padding\} y=\{padding\} width=\{chartSize\} height=\{chartSize\} fill=\{`url(#$\{isPdf ? "grid-pdf" : "grid"\})`\} />\cb1 \
\cb3                         \cb1 \
\cb3                         \{/* Quadrant Lines */\}\cb1 \
\cb3                         <line x1=\{padding\} y1=\{yScale(5)\} x2=\{width-padding\} y2=\{yScale(5)\} stroke=\{isPdf ? '#9ca3af' : 'currentColor'\} className=\{isPdf ? '' : 'text-neutral-400 dark:text-white/20'\} strokeWidth="2" strokeDasharray="5,5" />\cb1 \
\cb3                         <line x1=\{xScale(5)\} y1=\{padding\} x2=\{xScale(5)\} y2=\{height-padding\} stroke=\{isPdf ? '#9ca3af' : 'currentColor'\} className=\{isPdf ? '' : 'text-neutral-400 dark:text-white/20'\} strokeWidth="2" strokeDasharray="5,5" />\cb1 \
\cb3                         \cb1 \
\cb3                         \{/* Axes */\}\cb1 \
\cb3                         <line x1=\{padding\} y1=\{height-padding\} x2=\{width-padding\} y2=\{height-padding\} stroke=\{axisColor\} className=\{isPdf ? '' : 'text-neutral-600 dark:text-white/40'\} strokeWidth="2" />\cb1 \
\cb3                         <line x1=\{padding\} y1=\{padding\} x2=\{padding\} y2=\{height-padding\} stroke=\{axisColor\} className=\{isPdf ? '' : 'text-neutral-600 dark:text-white/40'\} strokeWidth="2" />\cb1 \
\cb3                         \cb1 \
\cb3                         \{/* Labels - Forced Inter Font */\}\cb1 \
\cb3                         <text x=\{width/2\} y=\{height-15\} textAnchor="middle" fill=\{textColor\} className=\{isPdf ? '' : 'text-neutral-700 dark:text-white/60'\} fontSize="14" fontWeight="bold" letterSpacing="0.05em" style=\{\{ fontFamily: 'Inter, sans-serif' \}\}>SATISFACTION</text>\cb1 \
\cb3                         <text x=\{25\} y=\{height/2\} textAnchor="middle" fill=\{textColor\} className=\{isPdf ? '' : 'text-neutral-700 dark:text-white/60'\} fontSize="14" fontWeight="bold" letterSpacing="0.05em" transform=\{`rotate(-90, 25, $\{height/2\})`\} style=\{\{ fontFamily: 'Inter, sans-serif' \}\}>IMPORTANCE</text>\cb1 \
\cb3                         \cb1 \
\cb3                         \{labelData.map((item, i) => \{\cb1 \
\cb3                             const isHovered = hoveredUnit === item.id;\cb1 \
\cb3                             \cb1 \
\cb3                             return (\cb1 \
\cb3                                 <g \cb1 \
\cb3                                     key=\{i\} \cb1 \
\cb3                                     onMouseEnter=\{() => !isPdf && setHoveredUnit(item.id)\}\cb1 \
\cb3                                     onMouseLeave=\{() => !isPdf && setHoveredUnit(null)\}\cb1 \
\cb3                                     style=\{\{ cursor: isPdf ? 'default' : 'pointer' \}\}\cb1 \
\cb3                                 >\cb1 \
\cb3                                     \{/* Pulse Effect Layer - Screen Only */\}\cb1 \
\cb3                                     \{!isPdf && isHovered && (\cb1 \
\cb3                                         <motion.circle\cb1 \
\cb3                                             cx=\{item.cx\} cy=\{item.cy\} r=\{item.r\}\cb1 \
\cb3                                             initial=\{\{ opacity: 0.5, scale: 1 \}\} animate=\{\{ opacity: 0, scale: 2 \}\} transition=\{\{ duration: 1, repeat: Infinity \}\}\cb1 \
\cb3                                             fill=\{item.color\}\cb1 \
\cb3                                         />\cb1 \
\cb3                                     )\}\cb1 \
\
\cb3                                     \{/* Connector Line */\}\cb1 \
\cb3                                     \{item.type === 'outside' && (\cb1 \
\cb3                                         <line x1=\{item.sx\} y1=\{item.sy\} x2=\{item.lx\} y2=\{item.ly\} \cb1 \
\cb3                                             stroke=\{isPdf ? 'black' : (isHovered ? item.color : (document.documentElement.classList.contains('dark') ? 'white' : 'black'))\} \cb1 \
\cb3                                             strokeWidth="1.5" \cb1 \
\cb3                                             opacity=\{isHovered ? 1 : (isPdf ? 1 : 0.3)\} \cb1 \
\cb3                                         />\cb1 \
\cb3                                     )\}\cb1 \
\cb3                                     \cb1 \
\cb3                                     \{/* Main Bubble */\}\cb1 \
\cb3                                     \{!isPdf ? (\cb1 \
\cb3                                         <motion.circle \cb1 \
\cb3                                             cx=\{item.cx\} cy=\{item.cy\} r=\{item.r\} fill=\{item.color\} opacity=\{isHovered ? 1 : 0.85\} \cb1 \
\cb3                                             stroke=\{bubbleStroke\} strokeWidth=\{isHovered ? 3 : 1.5\}\cb1 \
\cb3                                             animate=\{\{ scale: isHovered ? 1.2 : 1, filter: isHovered ? `drop-shadow(0 0 10px $\{item.color\})` : "none" \}\}\cb1 \
\cb3                                             transition=\{\{ type: "spring", stiffness: 300, damping: 20 \}\}\cb1 \
\cb3                                         />\cb1 \
\cb3                                     ) : (\cb1 \
\cb3                                         <circle \cb1 \
\cb3                                             cx=\{item.cx\} cy=\{item.cy\} r=\{item.r\} fill=\{item.color\} opacity="0.9" \cb1 \
\cb3                                             stroke="white" strokeWidth="1.5"\cb1 \
\cb3                                         />\cb1 \
\cb3                                     )\}\cb1 \
\
\cb3                                     \{/* Text Labels - Forced Inter Font */\}\cb1 \
\cb3                                     \{item.type === 'outside' ? (\cb1 \
\cb3                                         <text \cb1 \
\cb3                                             x=\{item.lx + item.xOffset\} y=\{item.ly\} dy="0.35em" textAnchor=\{item.anchor\} \cb1 \
\cb3                                             fill=\{isPdf ? '#111827' : 'currentColor'\} \cb1 \
\cb3                                             className=\{isPdf ? '' : `text-neutral-900 dark:text-white font-bold text-xs $\{isHovered ? 'opacity-100' : 'opacity-70'\}`\}\cb1 \
\cb3                                             fontSize=\{isPdf ? "12" : ""\} fontWeight="bold"\cb1 \
\cb3                                             style=\{\{ fontFamily: 'Inter, sans-serif' \}\}\cb1 \
\cb3                                         >\cb1 \
\cb3                                             \{item.name\}\cb1 \
\cb3                                         </text>\cb1 \
\cb3                                     ) : (\cb1 \
\cb3                                         <text \cb1 \
\cb3                                             x=\{item.cx\} y=\{item.cy\} dy="0.35em" textAnchor="middle" fill="white" \cb1 \
\cb3                                             fontSize=\{item.r > 35 ? "14" : "10"\} fontWeight="bold" \cb1 \
\cb3                                             className=\{isPdf ? '' : "pointer-events-none select-none shadow-black drop-shadow-md"\}\cb1 \
\cb3                                             style=\{\{ textShadow: isPdf ? 'none' : '0px 1px 3px rgba(0,0,0,0.8)', fontFamily: 'Inter, sans-serif' \}\}\cb1 \
\cb3                                         >\cb1 \
\cb3                                             \{item.name.split(' ')[0]\}\cb1 \
\cb3                                         </text>\cb1 \
\cb3                                     )\}\cb1 \
\cb3                                 </g>\cb1 \
\cb3                             );\cb1 \
\cb3                         \})\}\cb1 \
\cb3                     </svg>\cb1 \
\cb3                 </div>\cb1 \
\cb3             );\cb1 \
\cb3         \}\cb1 \
\
\cb3         // --- DATA STRUCTURE ---\cb1 \
\cb3         const lifePortfolioStructure = [\cb1 \
\cb3             \{ area: "Relationships", color: "#E34A4A", units: [\{ id: "significantOther", name: "Significant Other", desc: "Quality of relationship with partner/spouse." \}, \{ id: "family", name: "Family", desc: "Relationships with parents, siblings, children, etc." \}, \{ id: "friendship", name: "Friendship", desc: "Social circle and close friends." \}] \},\cb1 \
